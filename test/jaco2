pipeline {
  agent any
  tools {
    maven 'mymaven' 
  }
  stages {
    stage ('Build') {
      steps {
        sh 'mvn clean package'
      }
    }
    stage ('Deploy') {
      steps {
        sshagent(['ansadmin_jenkins']) {
          sh "scp -o StrictHostKeyChecking=no target/*.war ansadmin@3.143.255.251:/home/ansadmin/tomcat/webapps" 
}
      }
    }
  }
}
