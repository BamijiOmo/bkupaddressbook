pipeline {
    agent any
    tools {
   maven 'mymaven'
}
    stages {
        stage('Feb Cohort Build Job') {
            steps {
                sh 'mvn clean package'
            }
        }
stage('Feb Cohort Deploy Job') {
            steps {
                sshagent(['ansadminjenkins']) {
